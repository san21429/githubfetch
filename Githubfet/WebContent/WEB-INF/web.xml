<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Smart Bus</display-name>
  
<servlet>
<servlet-name>gitfetch</servlet-name>
<servlet-class>apps.Fetch</servlet-class>

</servlet>
<servlet>
    <servlet-name>fetch</servlet-name>
    <jsp-file>/NewFile.jsp</jsp-file>
</servlet>
<servlet>
    <servlet-name>tab</servlet-name>
    <jsp-file>/tablejsp.jsp</jsp-file>
</servlet>
  <servlet-mapping>
<servlet-name>gitfetch</servlet-name>
<url-pattern>/fetch/*</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>fetch</servlet-name>
<url-pattern>/fetchurl/*</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>tab</servlet-name>
<url-pattern>/table/*</url-pattern>
</servlet-mapping>


</web-app>